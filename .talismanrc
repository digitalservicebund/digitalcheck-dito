fileignoreconfig:
- filename: .env.example
  ignore_detectors: [filename]
- filename: .env.test
  ignore_detectors: [filename]
- filename: package-lock.json
  ignore_detectors: [filecontent]
- filename: app/root.tsx
  allowed_patterns: [posthogKey]
- filename: .github/workflows/pipeline.yml
  checksum: 67ac59339b7e352d70d4a236867f8a96d4f55d9d94b11455c3ebd89197e75aea
- filename: .github/workflows/scan.yml
  ignore_detectors: [filecontent]
allowed_patterns:
  - <[^>]+?\bkey=\{.*?\} # Allow <JSXElement ... key={...} patterns
  - :.+key:.+ # allow type definitions that have "key"
  - passen
  - Anpassungen
  - Pass\s # Allow "Pass " followed by a space
