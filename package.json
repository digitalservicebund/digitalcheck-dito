{
  "name": "digitalcheck-dito",
  "private": true,
  "homepage": "https://erarbeiten.digitalcheck.bund.de/",
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "audit:licences": "license-checker-rseidelsohn --production --excludePrivatePackages --onlyAllow 'MIT;BSD-3-Clause;BSD-2-Clause;0BSD;ISC;Apache-2.0;MPL-2.0;BlueOak-1.0.0;Custom: LICENSE;Unlicense' --summary",
    "build": "react-router build",
    "clean": "rm -rf build/",
    "dev": "react-router dev",
    "docker": "docker compose up --build",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write --ignore-unknown .",
    "lint:check": "react-router typegen && eslint --cache .",
    "lint:fix": "npm run lint:check -- --fix",
    "pre-commit": "npm run build && npm run style:fix && npm run tests && npm run typecheck",
    "start": "export $(grep -v '^#' .env | xargs) && react-router-serve build/server/index.js",
    "style:check": "npm run format:check && npm run lint:check",
    "style:fix": "npm run format:fix && npm run lint:fix",
    "test": "vitest",
    "test:a11y": "playwright test tests/a11y --config=tests/playwright.config.ts",
    "test:a11y-nightly": "playwright test tests/a11y --config=tests/playwright-nightly.config.ts",
    "test:e2e": "playwright test tests/e2e --config=tests/playwright.config.ts",
    "test:e2e-nightly": "playwright test tests/e2e --config=tests/playwright-nightly.config.ts",
    "test:snapshots": "playwright test tests/snapshots --config=tests/playwright-snapshots.config.ts",
    "test:update-snapshots": "playwright test tests/snapshots --config=tests/playwright-snapshots.config.ts --update-snapshots",
    "tests": "vitest run && npm run test:e2e && npm run test:a11y",
    "tests-nightly": "vitest run && npm run test:e2e-nightly && npm run test:a11y-nightly",
    "typecheck": "react-router typegen && tsc"
  },
  "dependencies": {
    "@digitalservice4germany/angie": "^2.0.1",
    "@digitalservice4germany/style-dictionary": "^2.0.0",
    "@digitalservicebund/icons": "^2.0.0",
    "@react-router/node": "^7.4.0",
    "@react-router/serve": "^7.4.0",
    "@rvf/react-router": "^7.0.0",
    "@rvf/zod": "^7.0.0",
    "@strapi/blocks-react-renderer": "^1.0.1",
    "@tailwindcss/vite": "^4.0.14",
    "isbot": "^5.1.4",
    "marked": "^15.0.0",
    "mime-types": "^2.1.35",
    "msw": "^2.7.0",
    "node-cache": "^5.1.2",
    "pdf-lib": "^1.17.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^7.4.0",
    "tailwind-merge": "^3.1.0",
    "unleash-client": "^6.6.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.0",
    "@commitlint/types": "^19.5.0",
    "@eslint/compat": "^1.2.7",
    "@eslint/js": "^9.22.0",
    "@playwright/test": "^1.50.1",
    "@react-router/dev": "^7.4.0",
    "@react-router/fs-routes": "^7.4.0",
    "@testing-library/jest-dom": "^6.6.1",
    "@testing-library/react": "^16.2.0",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^22.13.0",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.5",
    "autoprefixer": "^10.4.16",
    "dotenv": "^16.4.5",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-import-resolver-typescript": "^4.2.4",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-playwright": "^2.2.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-testing-library": "^7.0.0",
    "globals": "^16.0.0",
    "jsdom": "^26.0.0",
    "license-checker-rseidelsohn": "^4.4.2",
    "prettier": "^3.5.0",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.0.14",
    "ts-node": "^10.9.1",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.27.0",
    "vite": "^6.2.2",
    "vite-tsconfig-paths": "^5.1.0",
    "vitest": "^3.1.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
